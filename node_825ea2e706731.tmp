// Define a function that takes one integer argument and
// returns logical value true or false depending on if the integer is a prime.

function isPrime(num) {
    if (num <= 1) { return false }
    let divider = 0
    for (let i = 0; i <= num; i++){
        num % i == 0 ? divider++ : null
    }
    let result
    divider > 2 ? result = false : result = true
    return result
  }

console.log(isPrime(2100000000000000000000000000000000000))